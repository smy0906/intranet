{% extends 'ridi.twig' %}

{% block head %}
  {{ parent() }}

  <script>
    require(["jquery", "jquery-ui"], function () {
      if (location.search == '?after_login' && window.innerWidth < 980) {
        location.href = '/rooms';
      }
      else {
        location.href = '/posts/notice';
      }
    });
  </script>

{% endblock %}
